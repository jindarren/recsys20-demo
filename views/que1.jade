        script(src = "../bootstrap/dist/js/bootstrap.min.js")
        script(type='text/javascript').
            var platform = window.localStorage.getItem("platform")
            if(platform=="turk"){
                $("#user-id").text("MTurk Worker ID:")
            }else if(platform=="prolific"){
                $("#user-id").text("Prolific ID:")
            }

            var formList = [null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null] //33
            $("li > input").click(function () {
                var statement = parseInt($(this).parent().parent().attr("id").replace(/[^0-9]/ig, ""))
                var option = parseInt($(this).attr("value"))
                console.log(option)
                formList[statement + 3] = option
                console.log(formList)
            })
            $(".clear").click(function (event) {
                $("input").checked = false;
                formList = [null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null] //33
            })
            $(".submit").click(function (event) {
                event.preventDefault()
                var workerId = $("#workerID").val();
                if (workerId != "")
                    formList[0] = workerId
                var age = $("#age option:selected").text();
                if (age != "")
                    formList[1] = age
                var gender = $("#gender option:selected").text();
                if (gender != "")
                    formList[2] = gender
                var nationality = $("#nationality option:selected").text();
                if (nationality != "")
                    formList[3] = nationality
                console.log(formList)
                var uncheckedList = []
                $(".statement").css("background-color", "white")
                for (var index in formList) {
                    if (formList[index] == null) {
                        uncheckedList.push(index)
                        $(".statement:eq(" + index + ")").css("background-color", "yellow")
                    }
                }

                console.log(uncheckedList)
                if (uncheckedList.length > 0) {
                    alert("Please finished the missing statement(s) marked in yellow.")
                } else {
                    window.localStorage.setItem("que1",formList.toString())

                    var log = {
                        id: window.localStorage.getItem("userid"),
                        que1: formList,
                        que1Timestamp: new Date()
                    }

                    $.ajax({
                        url: '/updateRecord',
                        type: 'POST',
                        contentType: 'application/json',
                        data: JSON.stringify(log),
                        dataType: 'json',
                        success: function (data) {
                            console.log(data)
                            //var redirectedLink = window.localStorage.getItem("setting")
                            window.location.href = "/tutorial"
                        },
                        error: function (err) {
                            console.log(err)
                        }
                    });
                }
            })
