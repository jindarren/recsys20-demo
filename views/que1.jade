    script(type='text/javascript').
      var formList = [null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null]
      $("li > input").click(function(){
      var statement = parseInt($(this).parent().parent().attr("id").replace(/[^0-9]/ig,""))
      var option = parseInt($(this).attr("value"))
      console.log(option)
      formList[statement+2]=option
      console.log(formList)
      })
      $(".clear").click(function(event){
      $("input").checked=false;
      $("#post-20").empty()
      formList = [null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null]
      })
      $(".submit").click(function(event){
      event.preventDefault()
      var age = $( "#age option:selected" ).text();
      if(age!="")
      formList[0]=age
      var gender = $( "#gender option:selected" ).text();
      if(gender!="")
      formList[1]=gender
      var nationality = $("#nationality").val()
      if(nationality!="")
      formList[2]=nationality
      console.log(formList)
      var uncheckedList = []
      $(".statement").css("background-color","white")
      for(var index in formList){
      if(formList[index]==null){
      uncheckedList.push(index)
      $(".statement:eq("+index+")").css("background-color","yellow")
      }
      }
      console.log(uncheckedList)
      if(uncheckedList.length>0){
      alert("Please finished the missing statement(s) marked in yellow.")
      }else{
      window.location.href="/index" 
      }
      })
