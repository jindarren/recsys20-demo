doctype html
html
    head
        title Questionnaire
        style(type='text/css').
            html, body {
                padding: 0;
                margin: 0;
                font-family: Lato;
            }

            .clear, .submit {
                margin-left: 10px;
                margin-right: 10px;
            }

            .wrap, select {
                font-size: 16px;
            }

            h5.likert-header {
                text-align: center;
                margin: 20px 0 0;
            }

            form {
                width: 70%;
                margin-left: 15%;
            }

            p#description {
                margin: 10px 15% 0;
            }

            form .normal-input {
                margin-left: 8%;
            }

            form .statement {
                display: block;
                font-size: 18px;
                padding: 30px 0 0 4.25%;
                margin-bottom: 10px;
            }

            form .likert {
                list-style: none;
                width: 100%;
                left: 10%;
                margin: 0;
                padding: 50px 0 50px;
                display: block;
                border-bottom: 1px solid #efefef;
            }

            form .likert:last-of-type {
                border-bottom: 0;
            }

            form .likert:before {
                content: '';
                position: relative;
                top: 22px;
                display: block;
                background-color: #efefef;
                height: 4px;
                width: 95%;
            }

            form .likert li {
                display: inline-block;
                width: 13%;
                text-align: center;
                vertical-align: top;
                font-weight: lighter;
            }

            form .likert li input[type=radio] {
                display: block;
                position: relative;
                top: 0;
                left: 50%;
                margin-left: -6px;
            }

            form .likert li label {
                width: 100%;
            }

            form .buttons {
                margin: 100px;
                text-align: center;
            }

            textarea {
                margin-left: 2%;
                font-size: 16px;
                width: 90%;
            }

            li > input {
                zoom: 3;
            }

            .required {
                color: #f51616;
                font-weight: bold;
            }
